<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="327.0" prefWidth="553.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.DetailRapportController">
    <Label prefHeight="39.0" prefWidth="406.0" text="Détail Rapport Médical " textFill="STEELBLUE">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <font>
         <Font name="System Bold" size="14.0" />
      </font></Label>
     <HBox spacing="30.0">
      <Label lineSpacing="30.0" prefHeight="20.0" prefWidth="149.0" text="Nom Animal :" textFill="STEELBLUE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <Label lineSpacing="30.0" prefHeight="20.0" prefWidth="125.0" text="     Age :" textFill="STEELBLUE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <Label fx:id="nom" prefHeight="20.0" prefWidth="181.0" text="          Poids :" textFill="STEELBLUE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label lineSpacing="30.0" prefHeight="20.0" prefWidth="264.0" text="Description :" textFill="STEELBLUE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
    </HBox>
    <HBox prefHeight="20.0" prefWidth="528.0" spacing="30.0">
      <Label fx:id="nomanimal" prefHeight="17.0" prefWidth="145.0" style="-fx-background-radius: 10; -fx-border-color: #4682B4; -fx-border-radius: 10; -fx-background-color: #ffffff;" />
      <Label fx:id="age" prefHeight="20.0" prefWidth="122.0" style="-fx-background-radius: 10; -fx-border-color: #4682B4; -fx-border-radius: 10; -fx-background-color: #ffffff;" />
      <Label fx:id="poids" prefHeight="20.0" prefWidth="117.0" style="-fx-background-radius: 10; -fx-border-color: #4682B4; -fx-border-radius: 10; -fx-background-color: #ffffff;" />
      <Label fx:id="email" prefHeight="20.0" prefWidth="255.0" style="-fx-background-radius: 10; -fx-border-color: #4682B4; -fx-border-radius: 10; -fx-background-color: #ffffff;" />
    </HBox>
   <HBox prefHeight="34.0" prefWidth="541.0">
      <children>
         <Label prefHeight="30.0" prefWidth="136.0" text="Prénom Propriétaire :" textFill="STEELBLUE">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
         <Label prefHeight="30.0" prefWidth="136.0" text="Nom Du Propriétaire :" textFill="STEELBLUE">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </children>
   </HBox>
   <HBox prefHeight="39.0" prefWidth="541.0">
      <children>
         <Label fx:id="nomveto" prefHeight="19.0" prefWidth="130.0" style="-fx-background-radius: 10; -fx-border-color: #4682B4; -fx-border-radius: 10; -fx-background-color: #ffffff;" />
         <Label fx:id="prenomveto" prefHeight="17.0" prefWidth="145.0" style="-fx-background-radius: 10; -fx-border-color: #4682B4; -fx-border-radius: 10; -fx-background-color: #ffffff;" />
      </children>
   </HBox>
    <ImageView fx:id="rapport" fitHeight="53.0" fitWidth="63.0" layoutX="180.0" onMouseClicked="#Rapportlecharger" pickOnBounds="true" preserveRatio="true">
      <VBox.margin>
         <Insets right="12.0" />
      </VBox.margin>
      <image>
         <Image url="@../images/telecharger.png" />
      </image>
   </ImageView>
   <padding>
      <Insets left="12.0" />
   </padding>
</VBox>
